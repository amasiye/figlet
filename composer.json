{
  "name": "amasiye/figlet",
  "type": "library",
  "description": "Figlet text generator - PHP",
  "keywords": ["github", "figlet", "ascii", "text", "php"],
  "license": "MIT",
  "authors": [
    {
      "name": "Povilas Susinskas",
      "email": "povilassusinskas@gmail.com"
    },
    {
      "name": "Andrew Masiye",
      "email": "amasiye313@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/console": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Amasiye\\Figlet\\": "src/Figlet"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Amasiye\\Figlet\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "pestphp/pest": "^2.34"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "vendor/bin/pest tests/Unit/"
  }
}
